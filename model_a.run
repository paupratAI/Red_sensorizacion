# Cargar archivos 
model model_a.mod;
data model_a.dat;

# Resolver el problema  
option solver cplex;  
solve;

# Mostrar los resultados de las variables y objetivos principales
display x;
display c;
display pf;

# Mostrar el valor de la funciÃ³n objetivo
display TotalFlow;

# Abrir archivo para guardar los resultados
option display_output "output.txt";

# Guardar los resultados en un archivo
printf: "Results of x (intersections with sensors):\n";
printf {i in INTERSECTIONS: x[i] >= 1} "%s\n", i;

# Cerrar archivo
option display_output "";